{
  "name": "marked-typograf",
  "version": "0.0.1",
  "description": "marked extension for typograf",
  "main": "./lib/index.cjs",
  "module": "./lib/index.mjs",
  "browser": "./lib/index.umd.js",
  "type": "module",
  "keywords": [
    "marked",
    "extension",
    "typograf",
    "typography"
  ],
  "files": [
    "lib/",
    "src/"
  ],
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs"
    }
  },
  "scripts": {
    "build": "bun esbuild.config.js",
    "format": "eslint --fix",
    "lint": "eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laidrivm/marked-typograf.git"
  },
  "author": "Vladimir Lazarev <ladirvm@gmail.com> (https://laidrivm.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/laidrivm/marked-typograf/issues"
  },
  "homepage": "https://github.com/laidrivm/marked-typograf#readme",
  "peerDependencies": {
    "marked": ">=15"
  },
  "devDependencies": {
    "@markedjs/eslint-config": "^1.0.12",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "eslint": "^9.23.0",
    "marked": "^15.0.7",
    "semantic-release": "^24.2.3"
  },
  "dependencies": {
    "esbuild": "^0.25.1"
  }
}
